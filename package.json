{
  "name": "vscode-mdx-include",
  "displayName": "Mdx Include File",
  "description": "Lints and provides navigation for mdx file references in markdown code blocks",
  "version": "0.3.6",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Linters",
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "package": "vsce package"
  },
  "keywords": [
    "mkdocs",
    "markdown",
    "linter",
    "navigation"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zmievsa/vscode-mdx-include.git"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/vscode": "^1.60.0",
    "@vscode/vsce": "^3.2.2",
    "typescript": "^5.4.2"
  }
}